#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    for(int i = 0; i<4; i++){
    int n;
    cin>>n;
    int x = n;
    int sum = 0;
    int k = to_string(n).size();
    while(n){
        int l = n%10;
        sum = sum + pow(l, k);
        n = n/10;
    }
    if(sum == x) cout<<" "<< "true"<<endl;
    else cout<<" "<< "false"<<endl;
    }
    return 0;
}
